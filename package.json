{
  "name": "checkride",
  "version": "1.0.0",
  "engines": {
    "node": "14.x"
  },
  "description": "",
  "main": "index.ts",
  "jest": {
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/*.test.ts"
    ],
    "verbose": true
  },
  "scripts": {
    "start": "ts-node app/app.ts",
    "start:npx": "npx ts-node app/app.ts",
    "test": "jest",
    "dev": "nodemon",
    "test:coverage": "npx jest --coverage",
    "test:default": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "ibm-watson": "^6.2.1",
    "moment": "^2.29.1",
    "multer": "^1.4.3"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.11.11",
    "@types/supertest": "^2.0.11",
    "jest": "^27.4.3",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  }
}
